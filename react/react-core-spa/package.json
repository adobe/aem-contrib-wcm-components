{
  "name": "aem-core-components-contributions-react-core-spa",
  "version": "0.1.6",
  "scripts": {
    "linter": "eslint --ext .js,.jsx .",
    "linter:fix": "eslint --ext .js,.jsx . --fix",
    "test": "npm run build && cross-env NODE_ENV=test karma start --browsers=ChromeHeadless --single-run=true",
    "test:debug": "npm run build && cross-env NODE_ENV=test karma start --browsers=Chrome_with_debugging",
    "build": "webpack && tsc -p src/tsconfig.types.json",
    "build:production": "cross-env NODE_ENV=production webpack --mode=production &&  tsc -p src/tsconfig.types.json",
    "build:types": "tsc -p src/tsconfig.types.json",
    "docs": "jsdoc -c ./jsdoc_conf.json && doxdox \"src/**/*.+(js|jsx)\" --layout templates/DOCUMENTATION.hbs --output DOCUMENTATION.md",
    "readme": "node node_modules/markdown-include/bin/cli.js markdown-include.config.json",
    "docs-readme": "npm run docs && npm run readme",
    "preversion": "npm run test && npm run build:production && npm run docs-readme",
    "version": "npm run docs-readme && git add DOCUMENTATION.md README.md package.json",
    "postversion": "git push origin && git push origin --tags",
    "tmprelease": "node release.js"
  },
  "author": "Adobe Systems Inc. - Niek Raaijmakers",
  "homepage": "https://github.com/adobe/aem-contrib-wcm-components/blob/feature/react-wrapper/react/react-core-spa/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/aem-contrib-wcm-components/blob/feature/react-wrapper/react/react-core-spa"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-responsive-carousel": "^3.2.4",
    "sprintf": "^0.1.5"
  },
  "peerDependencies": {
    "aem-core-components-contributions-react-core": "^0.1.10",
    "@adobe/cq-react-editable-components": "^1.2.0",
    "@adobe/cq-spa-component-mapping": "^1.0.3",
    "@adobe/cq-spa-page-model-manager": "^1.1.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "aem-core-components-contributions-react-core": "^0.1.11",
    "@adobe/cq-react-editable-components": "^1.2.0",
    "@adobe/cq-spa-component-mapping": "^1.0.3",
    "@adobe/cq-spa-page-model-manager": "^1.1.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "babel-loader": "^8.0.6",
    "@babel/node": "^7.8.7",
    "babel-plugin-istanbul": "^5.2.0",
    "chai": "^4.2.0",
    "karma": "^4.4.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.1",
    "karma-mocha": "^1.3.0",
    "karma-sinon": "^1.0.5",
    "karma-sinon-chai": "^2.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.2",
    "markdown-include": "^0.4.3",
    "mocha": "^6.2.2",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.3.0",
    "istanbul": "^0.4.5",
    "istanbul-instrumenter-loader": "^3.0.1",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^6.0.3",
    "doxdox": "^3.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-react": "^7.17.0",
    "jsdoc": "^3.6.3",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "typescript": "^3.8.3",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
